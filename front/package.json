{
  "author": "dbo",
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "concurrently": "^4.1.1",
    "diff": "^4.0.1",
    "electron-dev": "^1.0.1",
    "electron-is-dev": "^1.1.0",
    "electron-tabs": "^0.9.5",
    "git-diff": "^2.0.6",
    "jsonpath-plus": "^0.20.1",
    "moment": "^2.24.0",
    "n-readlines": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-responsive": "^7.0.0",
    "react-scripts": "3.0.1",
    "react-xml-parser": "^1.1.2",
    "react-xterm": "^2.0.4",
    "text-file-diff": "^1.0.6",
    "wait-on": "^3.2.0",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.19",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27",
    "xterm": "^3.14.4"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"BROWSER=none npm run-script react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "webpack-cli": "^3.3.6",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "typescript": "3.5.3"
  }
}
